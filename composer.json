{
  "name": "toastnz/blocks-layouts",
  "description": "Silverstripe content blocks module",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "content blocks",
    "layouts",
    "modular"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
    "name": "Priscilla Yap",
    "homepage": "http://toast.co.nz/",
    "role": "Developer"
    },
    {
      "name": "Emmanuel Davies",
      "homepage": "http://toast.co.nz/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "silverstripe/cms": "^5",
    "silverstripe/framework": "^5",
    "composer/installers": "*",
    "symbiote/silverstripe-gridfieldextensions": "^4",
    "toastnz/linkable": "^3",
    "undefinedoffset/sortablegridfield": "^2.0.4",
    "axllent/silverstripe-form-fields": "^1.5",
    "silverstripe/userforms": "^6",
    "toastnz/open-cms-preview": "^0.0.10"
  },
  "extra": {
    "resources-dir": "_resources",
    "expose": [
        "client/images",
        "client/dist"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
